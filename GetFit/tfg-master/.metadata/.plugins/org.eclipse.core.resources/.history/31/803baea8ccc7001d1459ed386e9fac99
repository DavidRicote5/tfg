package org.agaray.spring.pap2023.controllers.rest;

import java.util.List;

import org.agaray.spring.pap2023.entities.Aficion;
import org.agaray.spring.pap2023.services.AficionService;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

@RestController
@RequestMapping("/api/aficion")
public class AficionRestController {

	@Autowired
	private AficionService aficionService;
	
	@GetMapping("r")
	public List<Aficion> rGet() {
		return aficionService.getAficiones();
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
